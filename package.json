{
  "name": "sample-project",
  "version": "1.0.0",
  "description": "Sample project structure for the IQVIA NextGent backend microservices",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "npx tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "lint": "npx eslint src --ext .ts",
    "lint:fix": "npx eslint src --ext .ts --fix",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "clean": "npx rimraf dist",
    "prebuild": "npm run clean",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "build:prod": "NODE_ENV=production npm run build",
    "prepare": "npx husky",
    "db:seed": "npx ts-node src/scripts/load-data/index.ts",
    "db:client": "npx prisma generate",
    "db:deploy": "npx prisma migrate deploy",
    "prisma:seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "keywords": [
    "IQVIA",
    "NextGen",
    "Backend",
    "Node.js"
  ],
  "author": "Oleksii Starovoitenko",
  "license": "ISC",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.1.1",
    "@aws-sdk/client-s3": "^3.614.0",
    "@prisma/client": "^5.22.0",
    "axios": "^1.7.2",
    "config": "^3.3.12",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf-csrf": "^3.0.6",
    "csv-parse": "^5.6.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "inversify": "^6.0.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openid-client": "^5.6.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.1",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/config": "^3.3.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/inversify": "^2.0.33",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.14.11",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
